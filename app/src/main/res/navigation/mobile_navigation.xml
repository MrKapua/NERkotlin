<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_find"
        android:name="com.kapuaStudio.ner.ui.buscar.FindFragment"
        android:label="@string/title_find"
        tools:layout="@layout/fragment_find" />
    <fragment
        android:id="@+id/navigation_radar"
        android:name="com.kapuaStudio.ner.ui.radar.RadarFragment"
        android:label="@string/title_radar"
        tools:layout="@layout/fragment_radar" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.kapuaStudio.ner.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.kapuaStudio.ner.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.kapuaStudio.ner.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>